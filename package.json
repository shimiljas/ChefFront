{
  "name": "ChefOrder",
  "version": "0.0.1",
  "private": true,
  "description": "React Native and Redux Chefs or Caterers Booking App.",
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "bundle-ios": "node node_modules/react-native/local-cli/cli.js bundle --entry-file index.ios.js --bundle-output iOS/main.jsbundle --platform 'ios' --assets-dest ./assets  --dev false --reset-cache",
    "bundle-android": "node node_modules/react-native/local-cli/cli.js bundle --entry-file index.android.js --bundle-output iOS/main.jsbundle --platform 'android' --assets-dest ./  --dev false --reset-cache",
    "android-port": "adb reverse tcp:8081 tcp:8080",
    "codepush-ios": "code-push release-react ChefOrder ios",
    "codepush-ios-beta": "code-push release-react ChefOrder ios --deploymentName Beta",
    "promote-ios-beta": "code-push promote ChefOrder Staging Beta",
    "codepush-android": "code-push release-react ChefOrder android",
    "codepush-android-beta": "code-push release-react ChefOrder android --deploymentName Beta",
    "promote-android-beta": "code-push promote ChefOrder Staging Beta",
    "android-build": "cd android && ./gradlew clean && ./gradlew assembleRelease",
    "run-android": "react-native run-android",
    "run-ios": "react-native run-ios",
    "reset": "watchman watch-del-all && npm cache clean",
    "uninstall": "rm -rf node_modules && npm install",
    "reload": "adb shell input keyevent 82",
    "enable-remotedev-debugger": "remotedev-debugger --hostname localhost --port 5678 --injectserver",
    "internet-on": "sudo ifconfig en0 up",
    "internet-off": "sudo ifconfig en0 down"
  },
  "dependencies": {
    "crypto-js": "^3.1.9-1",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "mongoid-js": "^1.1.2",
    "prop-types": "^15.5.10",
    "querystring": "^0.2.0",
    "react": "16.0.0-alpha.12",
    "react-mixin": "^2.0.2",
    "react-native": "0.45.1",
    "react-native-actionsheet": "^2.2.2",
    "react-native-animatable": "^1.2.2",
    "react-native-calendar": "^0.12.3",
    "react-native-calendars": "^1.5.6",
    "react-native-chart": "^1.0.8-beta",
    "react-native-code-push": "^3.0.0-beta",
    "react-native-credit-card-input": "^0.3.3",
    "react-native-device-info": "^0.10.2",
    "react-native-fcm": "^7.5.1",
    "react-native-geocoder": "^0.4.8",
    "react-native-gifted-chat": "^0.2.5",
    "react-native-google-places-autocomplete": "^1.2.12",
    "react-native-image-crop-picker": "^0.13.0",
    "react-native-in-app-notification": "^1.0.3",
    "react-native-interactable": "0.0.9",
    "react-native-keyevent": "0.0.7",
    "react-native-loading-spinner-overlay": "^0.5.0",
    "react-native-meteor": "^1.0.6",
    "react-native-mov-to-mp4": "^0.2.1",
    "react-native-navbar": "^1.7.1",
    "react-native-pathjs-charts": "0.0.30",
    "react-native-permissions": "^0.2.7",
    "react-native-photo-view": "^1.4.0",
    "react-native-redux-toast": "^1.0.0",
    "react-native-scrollable-tab-view": "^0.6.7",
    "react-native-simple-modal": "^6.0.0",
    "react-native-stripe-api": "^0.1.0",
    "react-native-tag-input": "0.0.14",
    "react-native-thumbnail": "^1.0.8",
    "react-native-timer": "^1.3.1",
    "react-native-underline-tabbar": "^1.0.6",
    "react-native-vector-icons": "^4.2.0",
    "react-native-video": "^1.0.0",
    "react-native-video-player": "^0.7.0",
    "react-navigation": "^1.0.0-beta.11",
    "react-redux": "^5.0.5",
    "react-timer-mixin": "^0.13.3",
    "redux": "^3.7.1",
    "redux-persist": "^4.8.2",
    "redux-side-effect": "^2.1.2",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-jest": "20.0.3",
    "babel-preset-react-native": "2.0.0",
    "jest": "20.0.4",
    "react-devtools": "^2.4.0",
    "react-test-renderer": "16.0.0-alpha.12",
    "redux-devtools": "^3.4.0",
    "redux-devtools-extension": "^2.13.2",
    "remote-redux-devtools": "^0.5.12",
    "remote-redux-devtools-on-debugger": "^0.7.1",
    "remotedev-rn-debugger": "^0.8.2"
  },
  "jest": {
    "preset": "react-native"
  }
}
